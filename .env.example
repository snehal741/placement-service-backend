# Environment
NODE_ENV=production

# Server Configuration
PORT=3001

# CORS Configuration
ALLOWED_ORIGINS=https://mauliplacements.com,https://www.mauliplacements.com

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
GOOGLE_REFRESH_TOKEN=your_refresh_token
GOOGLE_DRIVE_FOLDER_ID=your_folder_id

# OAuth Redirect URL (production)
OAUTH_REDIRECT_URL=https://api.mauliplacements.com/auth/google/callback

# Rate Limiting (optional, defaults exist)
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX_REQUESTS=100  # requests per window
